<h1>ここはindexページです</h1>
<h1>Books</h1>

<h2>New book</h2>
<%= form_with model: @book do |f| %>
  <div class = form>
    <p>Title</p>
    <%= f.text_field :title %>
  </div>
  
  <div class = form>
    <p>Body</p>
    <%= f.text_area :body %>
  </div>
  
  <div class = form>
    <%= f.submit 'Create Book' %>
  </div>
<% end %>